<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.eci.pdsw.sampleprj.dao.mybatis.mappers.TipoUsuarioMapper">

	<resultMap type='TipoUsuario' id='TipoUsuarioResult'>
		<id property='id' column='id' />
		<result property='descripcion' column='descripcion' />
	</resultMap>

	<select id="consultarTiposUsuario" resultMap="TipoUsuarioResult">
		select * from
		tipo_usuario;
	</select>

	<select id="getTipoUsuario" resultMap="TipoUsuarioResult">
		select * from tipo_usuario
		where id = #{id};
	</select>


</mapper>